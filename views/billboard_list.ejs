<% if(data.subjects[0].subject) { data.subjects.forEach(function(item,index){ %>
						<li class="movie-list">
							<div class="movie-item clearfix">
							<i class="item-rank">
									<%= index+1 %>
								</i>
								<div class="item-left">
									<a href="movie/<%=item.subject.id %>" class="item-link">
										<img src="<%= item.subject.images.small %>" alt="">
									</a>
								</div>
								<div class="item-right">
									<div class="item-detail">
										<a href=""movie/<%=item.subject.id %>"">
										<span class="item-title">
										<%= item.subject.title %>
										</span>
										</a>
										<p class="item-casts">主演：
										<% item.subject.casts.forEach(function (item){ %>
										 <%= item.name %>
										<% })  %>
										</p>
										<p class="item-land">
										<% if(item.subject.mainland_pubdate!=""){%>
										上映时间<%= item.subject.mainland_pubdate %>
										<% } %>
										</p>
									</div>
									<div class="item-score">
										<span>
											<%= item.subject.rating.average %>
										</span>
									</div>
								</div>
							</div>
						</li>
					<%	})} else{ data.subjects.forEach(function(item,index){%>

			<li class="movie-list">
							<div class="movie-item clearfix">
							<i class="item-rank">
									<%= index+1 %>
								</i>
								<div class="item-left">
									<a href="movie/<%= item.id %>" class="item-link">
										<img src="<%= item.images.small %>" alt="">
									</a>
								</div>
								<div class="item-right">
									<div class="item-detail">
										<a href="movie/<%=item.id %>">
										<span class="item-title">
										<%= item.title %>
										</span>
										</a>
										<p class="item-casts">主演：
										<% item.casts.forEach(function(item){ %>
										 <%= item.name %>
										 <% }) %>
										</p>
										<p class="item-land">
										<% if(item.mainland_pubdate!=""){ %>
										上映时间:<%= item.mainland_pubdate %>
										<% } %>
										</p>
									</div>
									<div class="item-score">
										<span>
											<%= item.rating.average %>
										</span>
									</div>
								</div>
							</div>
						</li>
					<% })} %>

          